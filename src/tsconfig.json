{
  "compilerOptions": {
    "target": "ES2020",
    "lib": ["DOM", "ES2020"],
    "module": "ESNext",
    "moduleResolution": "Node",
    "jsx": "react-jsx",
    "strict": false,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "react": ["../client/node_modules/@types/react/index.d.ts"],
      "react/jsx-runtime": ["../client/node_modules/@types/react/jsx-runtime.d.ts"],
      "react/jsx-dev-runtime": ["../client/node_modules/@types/react/jsx-dev-runtime.d.ts"],
      "react-dom": ["../client/node_modules/@types/react-dom/index.d.ts"]
    }
  },
  "include": ["**/*.ts", "**/*.tsx"]
}
