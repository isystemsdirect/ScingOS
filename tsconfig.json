{
  "compilerOptions": {
    "target": "ES2020",
    "module": "CommonJS",
    "jsx": "preserve",
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmit": true
  },
  "include": ["scing/**/*.ts", "scing/**/*.tsx"],
  "exclude": [
    "src/**",
    "**/*.test.*",
    "node_modules",
    "client/.next/**",
    "client/out/**",
    "client/coverage/**"
  ]
}
