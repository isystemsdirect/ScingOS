{
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "eslint": "^8.0.0",
    "typescript": "5.3.3"
  },
  "overrides": {
    "typescript": "5.3.3",
    "@typescript-eslint/parser": "^5.62.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/utils": "^5.62.0",
    "@typescript-eslint/type-utils": "^5.62.0",
    "@typescript-eslint/types": "^5.62.0",
    "@typescript-eslint/typescript-estree": "^5.62.0",
    "@typescript-eslint/scope-manager": "^5.62.0",
    "@typescript-eslint/visitor-keys": "^5.62.0"
  },
  "scripts": {
    "typecheck:scing": "npx -y tsc -p tools/tmp-tsconfig.json",
    "lint:scing": "npx eslint \"scing/**/*.{ts,tsx}\" \"tools/**/*.{js,mjs}\" --max-warnings=0",
    "canon:scan": "node tools/canon-scan.mjs",
    "format:check": "npx prettier --check \"scing/**/*.{ts,tsx,js,mjs,json,md}\" \"tools/**/*.{js,mjs,json,md}\"",
    "quality:scing": "npm run typecheck:scing && npm run lint:scing && npm run canon:scan && npm run format:check"
  }
}
